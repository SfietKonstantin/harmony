<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
    <interface name="org.sfietkonstantin.harmony">
        <method name="RegisteredClients">
            <arg name="registeredClients" type="as" direction="out"/>
        </method>
        <method name="RegisterClient">
            <arg name="clientIp" type="s" direction="in"/>
            <arg name="password" type="s" direction="in"/>
            <arg name="registered" type="b" direction="out"/>
        </method>
        <method name="UnregisterClient">
            <arg name="clientIp" type="s" direction="in"/>
            <arg name="unregistered" type="b" direction="out"/>
        </method>
        <signal name="PasswordChanged" />
    </interface>
</node>
